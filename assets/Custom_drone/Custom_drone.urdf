    </inertial>
  </link>

  <!-- ==== Propeller joints ==== -->
  <joint name="propeller_4_joint" type="revolute">
    <parent link="drone_body"/>
    <child link="propeller_4"/>
    <origin xyz="0.0835 0.111 0.029" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14159" upper="3.14159" effort="0.1" velocity="50"/>
  </joint>

  <joint name="propeller_2_joint" type="revolute">
    <parent link="drone_body"/>
    <child link="propeller_2"/>
    <origin xyz="0.0835 -0.111 0.029" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14159" upper="3.14159" effort="0.1" velocity="50"/>
