<?xml version="1.0" ?>
<robot name="quadrotor">

  <!-- ==== Main frame ==== -->
  <link name="drone_body">
    <visual>
      <geometry>
        <mesh filename="meshes/Drone_frame_mesh.dae" scale="0.0006 0.0006 0.0006"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="meshes/Drone_frame_mesh.dae" scale="0.0006 0.0006 0.0006"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.631"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.00233681" ixy="0" ixz="0" iyy="0.00240461" iyz="0" izz="0.003746"/>
    </inertial>
  </link> 

  <!-- ==== Propellers ==== -->
  <link name="propeller_1">
    <visual>
      <geometry>
        <mesh filename="meshes/Propeller_mesh.dae" scale="0.0006 0.0006 0.0006"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="meshes/Propeller_mesh.dae" scale="0.0006 0.0006 0.0006"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.006"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-8" iyy="5.0e-6" izz="5.0e-6" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="propeller_2">
