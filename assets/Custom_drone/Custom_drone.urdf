      <mass value="0.006"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-8" iyy="5.0e-6" izz="5.0e-6" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="propeller_3">
    <visual>
      <geometry>
        <mesh filename="meshes/Propeller_mesh.dae" scale="0.0006 0.0006 0.0006"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
